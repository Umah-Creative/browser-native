{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@umah-creative/browser-native": ["packages/core/src"],
      "@umah-creative/browser-native/*": ["packages/core/src/*"]
    }
  }
}
